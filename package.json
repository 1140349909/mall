{
  "name": "mall",
  "version": "1.8.6",
  "description": "爆款平台",
  "author": "asoiso",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://git.vveshow.com/iloka/mall.git"
  },
  "keywords": [
    "linkin",
    "admin"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint src --ext .js --ext .jsx & exit 0",
    "lintx": "./node_modules/.bin/eslint src/h5 --ext .js --ext .jsx & exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "cd src && yarn install",
    "start": "npm run preinstall && fis3 server start --no-browse && fis3 release -r ./src -c -w",
    "webpack": "webpack-dev-server --hot --inline --progress",
    "debug": "npm run preinstall && fis3 server start --port 8062 --type node --www ./dist --no-browse && fis3 release -r ./src -c -w -L -d ./dist",
    "build-dev": "npm run preinstall && fis3 release testing -r ./src -c -u -d ./dist --no-color",
    "build-sit": "npm run preinstall && fis3 release testing -r ./src -c -u -d ./dist --no-color",
    "build-uat": "npm run preinstall && fis3 release testing -r ./src -c -u -d ./dist --no-color",
    "build-prd": "npm run preinstall && fis3 release production -r ./src -c -u -d ./dist --no-color"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "buffer": "4.6.0",
    "cropperjs": "^0.8.1",
    "es6-promise": "3.2.1",
    "is-buffer": "1.1.3",
    "lk-detect": "1.0.0",
    "lk-vuex": "1.0.0",
    "lodash": "^4.15.0",
    "process": "0.11.2",
    "vue": "1.0.28",
    "vue-router": "0.7.13",
    "vue-swipe": "^0.2.6",
    "vue-touch": "^1.1.0",
    "vux": "0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "babel-core": "~6.0.20",
    "babel-loader": "~6.0.1",
    "babel-plugin-import": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "~6.0.15",
    "babel-preset-react": "~6.0.15",
    "babel-preset-stage-0": "^6.16.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "~0.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "~0.8.4",
    "gulp": "^3.9.1",
    "history": "^1.17.0",
    "html-webpack-plugin": "2.x",
    "jsx-loader": "~0.13.2",
    "open-browser-webpack-plugin": "0.0.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "~0.12.3",
    "ts-loader": "^0.9.4",
    "typescript": "^2.0.3",
    "url-loader": "~0.5.6",
    "vue-loader": "^9.7.0",
    "webpack": "~1.13.0",
    "webpack-dev-server": "^1.14.0"
  },
  "main": "fis-conf.js",
  "license": "ISC"
}
